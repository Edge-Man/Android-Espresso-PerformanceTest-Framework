//HOTEL PAGE OBJECTS
    private static HotelBasePage hotelBasePage = null;

    public static HotelBasePage getHotelBasePage(){
        if(hotelBasePage == null){
            hotelBasePage = new HotelBasePage();
        }
        return hotelBasePage;
    }

    private static HotelDetailPage hotelDetailPage = null;

    public static HotelDetailPage getHotelDetailPage(){
